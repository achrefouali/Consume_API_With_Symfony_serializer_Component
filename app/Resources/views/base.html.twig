    <script language="JavaScript">
        function ajaxLoad(){
            $(document).ajaxStart(function() {
                $("#loading").show();
            }).ajaxStop(function() {
                $("#loading").hide();
            });
        }
        function checkIsLogged() {
            var url = "{{ auf['si']['url'] ~ '/api/user/'~ app.user.id ~ '.json?access_token=' ~ app.session.get('_security.api_access_token') }}";
            //console.log(url);
            $.get( url  )
                .done(function( data ) {
                    if(!data.hasOwnProperty('code')){
                        var isSILogged = data['users'][0]['isSILogged'];
                        var si_login_token = data['users'][0]['si_login_token'];

                        if(isSILogged == true || si_login_token == 'undefined'){
                            console.log('Bienvenue {{ app.user.firstname }}');
                        }else {
                            window.location = "{{ path('logout') }}";
                        }
                    }
                });
            //setTimeout( isLogged, 1000 );
        }
        $(document).ready(function () {
            ajaxLoad()
            $('.date').after("<i class='icon-calendar fa fa-calendar'></i>")
        })

    </script>
